WORKDIR = `pwd`

CC = gcc
CXX = g++
AR = ar
LD = g++
WINDRES = windres

INC = 
CFLAGS = -Wall -fexceptions
RESINC = 
LIBDIR = 
LIB = 
LDFLAGS = 

INC_USAGE = $(INC) -I../
CFLAGS_USAGE = $(CFLAGS) -std=c++11 -g
RESINC_USAGE = $(RESINC)
RCFLAGS_USAGE = $(RCFLAGS)
LIBDIR_USAGE = $(LIBDIR) -L../
LIB_USAGE = $(LIB)
LDFLAGS_USAGE = $(LDFLAGS)
OBJDIR_USAGE = obj/Debug
DEP_USAGE = 
OUT_USAGE = bin/Debug/LTestUse

OBJ_USAGE = $(OBJDIR_USAGE)/__/LTest.o $(OBJDIR_USAGE)/main.o

all: usage

clean: clean_usage

before_usage: 
	test -d bin/Debug || mkdir -p bin/Debug
	test -d $(OBJDIR_USAGE)/__ || mkdir -p $(OBJDIR_USAGE)/__
	test -d $(OBJDIR_USAGE) || mkdir -p $(OBJDIR_USAGE)

after_usage: 

usage: before_usage out_usage after_usage

out_usage: before_usage $(OBJ_USAGE) $(DEP_USAGE)
	$(LD) $(LIBDIR_USAGE) -o $(OUT_USAGE) $(OBJ_USAGE)  $(LDFLAGS_USAGE) $(LIB_USAGE)

$(OBJDIR_USAGE)/__/LTest.o: ../LTest.cpp
	$(CXX) $(CFLAGS_USAGE) $(INC_USAGE) -c ../LTest.cpp -o $(OBJDIR_USAGE)/__/LTest.o

$(OBJDIR_USAGE)/main.o: main.cpp
	$(CXX) $(CFLAGS_USAGE) $(INC_USAGE) -c main.cpp -o $(OBJDIR_USAGE)/main.o

clean_usage: 
	rm -f $(OBJ_USAGE) $(OUT_USAGE)
	rm -rf bin/Debug
	rm -rf $(OBJDIR_USAGE)/__
	rm -rf $(OBJDIR_USAGE)

.PHONY: before_usage after_usage clean_usage

